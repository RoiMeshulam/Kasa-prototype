{
  "cli": {
    "version": ">= 16.17.4",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "env": {
        "EXPO_PUBLIC_ENVIRONMENT": "development",
        "EXPO_PUBLIC_BACKEND_URL": "http://localhost:8080",
        "EXPO_PUBLIC_API_VERSION": "v1",
        "EXPO_PUBLIC_DEBUG_MODE": "true"
      }
    },
    "test": {
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "env": {
        "EXPO_PUBLIC_ENVIRONMENT": "test",
        "EXPO_PUBLIC_BACKEND_URL": "https://test-backend-393925206417.europe-west1.run.app",
        "EXPO_PUBLIC_API_VERSION": "v1",
        "EXPO_PUBLIC_DEBUG_MODE": "true"
      }
    },
    "preview": {
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "env": {
        "EXPO_PUBLIC_ENVIRONMENT": "test",
        "EXPO_PUBLIC_BACKEND_URL": "https://test-backend-393925206417.europe-west1.run.app",
        "EXPO_PUBLIC_API_VERSION": "v1",
        "EXPO_PUBLIC_DEBUG_MODE": "false"
      }
    },
    "production": {
      "autoIncrement": true,
      "android": {
        "buildType": "app-bundle"
      },
      "env": {
        "EXPO_PUBLIC_ENVIRONMENT": "production",
        "EXPO_PUBLIC_BACKEND_URL": "https://backend-393925206417.europe-west1.run.app",
        "EXPO_PUBLIC_API_VERSION": "v1",
        "EXPO_PUBLIC_DEBUG_MODE": "false"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
